
<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Kütüphane Sistemi</title>
    <link href="https://cdn.jsdelivr.net/npm/simple-datatables@latest/dist/style.css" rel="stylesheet" />
    <link href="~/Templates/startpanel/css/styles.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/js/all.min.js" crossorigin="anonymous"></script>
</head>
<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <!-- Navbar Brand-->
        <a class="navbar-brand ps-3" href="#">
            Kullanıcı Paneli
        </a>
        <!-- Sidebar Toggle-->
        <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>
        <!-- Navbar Search-->
        <form class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0">
        </form>
        <!-- Navbar-->
        <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
            <li class="nav-item active"><a class="nav-link" href="#">Hakkımızda</a></li>
            <li class="nav-item dropdown">

                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
                    <li><a class="dropdown-item" href="/Panel/Index/">Hesabım</a></li>
                    <li><hr class="dropdown-divider" /></li>
                    <li><a class="dropdown-item" asp-action="Logout" asp-controller="Member">Çıkış Yap</a></li>
                </ul>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <a class="nav-link" asp-action="UserEdit" asp-controller="Member">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Profilim
                        </a>
                        <a class="nav-link collapsed" asp-action="PasswordChange" asp-controller="Member">
                            <div class="sb-nav-link-icon"><i class="fas fa-box"></i></div>
                            Şİfre Değiştirme
                        </a>
                        @*<a class="nav-link collapsed" href="/Message/SerderMessage/">
                            <div class="sb-nav-link-icon"><i class="fas fa-box"></i></div>
                            Gönderilen Mesajlar
                        </a>
                        <a class="nav-link collapsed" href="/Message/NewMessage/">
                            <div class="sb-nav-link-icon"><i class="fas fa-box"></i></div>
                            Yeni Mesajlar
                        </a>
                        <a class="nav-link" href="/Panel/MyBook/">
                            <div class="sb-nav-link-icon"><i class="fas fa-book-reader"></i></div>
                            Alınan Kitaplar
                        </a>
                        <a class="nav-link" href="/Showcase/Index/">
                            <div class="sb-nav-link-icon"><i class="fas fa-table"></i></div>
                            Site Sayfası
                        </a>
                        <a class="nav-link" href="/Showcase/Index/#contact">
                            <div class="sb-nav-link-icon"><i class="fas fa-helicopter"></i></div>
                            Yardım
                        </a>*@
                    </div>
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                <div style="margin-left:5px">
                    @RenderBody()
                </div>
            </main>

        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/Templates/startpanel/js/scripts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>
    <script src="~/Templates/startpanel/assets/demo/chart-area-demo.js"></script>
    <script src="~/Templates/startpanel/assets/demo/chart-bar-demo.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/simple-datatables@latest" crossorigin="anonymous"></script>
    <script src="~/Templates/startpanel/js/datatables-simple-demo.js"></script>
</body>
</html>









